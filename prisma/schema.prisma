generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "darwin"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model crm_accounts {
  supaglue_application_id String    @map("_supaglue_application_id")
  supaglue_provider_name  String    @map("_supaglue_provider_name")
  supaglue_customer_id    String    @map("_supaglue_customer_id")
  supaglue_emitted_at     DateTime  @map("_supaglue_emitted_at")
  id                      String
  created_at              DateTime?
  updated_at              DateTime?
  is_deleted              Boolean
  last_modified_at        DateTime
  name                    String?
  description             String?
  industry                String?
  website                 String?
  number_of_employees     Int?
  addresses               Json?
  phone_numbers           Json?
  last_activity_at        DateTime?
  lifecycle_stage         String?
  owner_id                String?
  raw_data                Json?

  @@id([supaglue_application_id, supaglue_provider_name, supaglue_customer_id, id])
}

model crm_contacts {
  supaglue_application_id String    @map("_supaglue_application_id")
  supaglue_provider_name  String    @map("_supaglue_provider_name")
  supaglue_customer_id    String    @map("_supaglue_customer_id")
  supaglue_emitted_at     DateTime  @map("_supaglue_emitted_at")
  id                      String
  created_at              DateTime?
  updated_at              DateTime?
  is_deleted              Boolean
  last_modified_at        DateTime
  first_name              String?
  last_name               String?
  addresses               Json
  email_addresses         Json
  phone_numbers           Json
  lifecycle_stage         String?
  account_id              String?
  owner_id                String?
  last_activity_at        DateTime?
  raw_data                Json?
  email_address           String?   @default(dbgenerated("jsonb_path_query_first(email_addresses, '$[*]?(@.\"email_address_type\" == \"primary\").\"email_address\"'::jsonpath)"))

  @@id([supaglue_application_id, supaglue_provider_name, supaglue_customer_id, id])
}

